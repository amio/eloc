{
  "name": "eloc",
  "version": "1.2.2",
  "description": "1MB cli for markdown presentation authoring",
  "repository": "amio/eloc",
  "license": "MIT",
  "bin": "dist/index.js",
  "scripts": {
    "dev": "nodemon -w src -e js,ts -x npm run build",
    "prebuild": "rm -rf dist",
    "build": "ncc build src/index.ts -m",
    "now-build": "npm run build && node dist build deck.md -t 'Eloquence cli'",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.1.0",
    "@types/micromatch": "^4.0.1",
    "@types/mri": "^1.1.0",
    "@types/node": "^13.9.0",
    "@types/serve-handler": "^6.1.0",
    "@zeit/ncc": "^0.21.1",
    "fs-extra": "^9.0.0",
    "globby": "^11.0.0",
    "kleur": "^3.0.3",
    "markdown-deck": "^1.6.7",
    "micri": "^3.0.0",
    "micro-fork": "^3.0.0",
    "micromatch": "^4.0.2",
    "mri": "^1.1.4",
    "nodemon": "^2.0.2",
    "open": "^7.0.3",
    "serve-handler": "^6.1.2",
    "typescript": "^3.8.3"
  }
}
